    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "winbox": "^0.2.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@types/winbox": "^0.2.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "tsup": "6.0.1",
    "tsx": "^3.10.4",
    "typescript": "^4.7.4",
    "vue": "^3.2.45"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
